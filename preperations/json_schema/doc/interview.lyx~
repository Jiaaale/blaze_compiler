\begin_layout Subsection
CTL
\end_layout

\begin_layout BeginFrame
Verification
\end_layout

\begin_layout Itemize
Dynamic behavior of database even harder to predict
\end_layout

\begin_layout Itemize
Computational Tree Logic (CTL) is a formal language for describing temporal
 evolution of logical systems
\end_layout

\begin_deeper
\begin_layout Itemize
\begin_inset Formula $\mathbf{A}$
\end_inset

 means all paths
\end_layout

\begin_layout Itemize
\begin_inset Formula $\mathbf{\mathbf{E}}$
\end_inset

 means there exists a path
\end_layout

\begin_layout Itemize
\begin_inset Formula $\mathbf{G}$
\end_inset

 global modifier (always true)
\end_layout

\begin_layout Itemize
\begin_inset Formula $\mathbf{F}$
\end_inset

 finally modifier (the path leads to ...)
\end_layout

\end_deeper
\begin_layout Itemize
SPEC AG EF users.fred.state = IDLE
\end_layout

\begin_layout Itemize
SPEC !EF (users.john.item = gold & users.fred.item = gold)
\end_layout

\begin_layout EndFrame

\end_layout

\begin_layout BeginFrame
Verification
\end_layout

